              <body>logger.log(LogLevel.INFO, &quot;Connecting...&quot;);
		try {
			otherConnector.sendMessage(
					new ConnectorDataPacket&lt;ISyncPeersMsg>(new SyncPeersMsg(connectedUsersSet), myNamedConnector));
		} catch (RemoteException e) {
			e.printStackTrace();
