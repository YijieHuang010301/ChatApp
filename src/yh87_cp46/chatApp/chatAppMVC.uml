            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0HTMfl3XEeybfsNPaLlcrQ" name="joinChatroom" specification="_0Gq6Ul3XEeybfsNPaLlcrQ">
              <ownedParameter xmi:id="_0HTMf13XEeybfsNPaLlcrQ" name="chatRoomName">
