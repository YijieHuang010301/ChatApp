            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0HTMhl3XEeybfsNPaLlcrQ" name="quit" specification="_z4zdcF3XEeybfsNPaLlcrQ">
              <language>java</language>
              <body>try {
			for (INamedConnector eachConnector : connectedUsersSet)
				eachConnector.sendMessage(new ConnectorDataPacket&lt;IQuitMsg>(new QuitMsg(), myNamedConnector));
