              <language>java</language>
              <body>new Thread(() -> {
			try {
				remoteHostStub.sendMessage(
						new ConnectorDataPacket&lt;IInviteMsg>(new InviteMsg(this.myModel2ViewAdapter.getRooms()), myNamedConnector));
